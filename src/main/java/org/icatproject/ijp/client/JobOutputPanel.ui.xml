<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	<ui:style src="portal.css" />
	<g:VerticalPanel>
		<g:ScrollPanel ui:field='jobOutputScrollPanel' height='500px' width='800px'>
			<g:HTMLPanel>
				<pre>
					<g:Label ui:field='jobOutputLabel'></g:Label>
				</pre>
			</g:HTMLPanel>
		</g:ScrollPanel>
		<g:HTMLPanel>
			<hr></hr>
			<center>
				<g:Button ui:field="closeButton" text='Close' />
			</center>
		</g:HTMLPanel>
	</g:VerticalPanel>
</ui:UiBinder>