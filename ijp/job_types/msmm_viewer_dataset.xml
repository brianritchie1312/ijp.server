<jobType>
    <name>MSMM Viewer Dataset</name>
    <executable>/usr/local/msmm/bin/msmm_viewer_dataset</executable>
    <multiple>false</multiple>
    <type>interactive</type>
    <datasetTypes>dataset</datasetTypes>
    <jobOptions>
        <name>View</name>
        <groupName>View type</groupName>
        <type>boolean</type>
        <programParameter></programParameter>
        <condition></condition>
    </jobOptions>
    <jobOptions>
        <name>View (full frame)</name>
        <groupName>View type</groupName>
        <type>boolean</type>
        <programParameter>--option=fullframe</programParameter>
        <condition></condition>
    </jobOptions>
    <jobOptions>
        <name>View (full frame, overlay channels)</name>
        <groupName>View type</groupName>
        <type>boolean</type>
        <programParameter>--option=fullframe-overlay</programParameter>
        <condition></condition>
    </jobOptions>
    <jobOptions>
        <name>View beads</name>
        <groupName>View type</groupName>
        <type>boolean</type>
        <programParameter>--beads</programParameter>
        <condition>beadDatasetId!=null</condition>
    </jobOptions>
    <jobOptions>
        <name>View beads (full frame)</name>
        <groupName>View type</groupName>
        <type>boolean</type>
        <programParameter>--option=beads-fullframe</programParameter>
        <condition>beadDatasetId!=null</condition>
    </jobOptions>
    <jobOptions>
        <name>View beads (full frame, overlay channels)</name>
        <groupName>View type</groupName>
        <type>boolean</type>
        <programParameter>--option=beads-fullframe-overlay</programParameter>
        <condition>beadDatasetId!=null</condition>
    </jobOptions>
    <jobOptions>
        <name>View flats</name>
        <groupName>View type</groupName>
        <type>boolean</type>
        <programParameter>--flats</programParameter>
        <condition>flatfieldDatasetId!=null</condition>
    </jobOptions>
    <jobOptions>
        <name>View flats (full frame)</name>
        <groupName>View type</groupName>
        <type>boolean</type>
        <programParameter>--option=flats-fullframe</programParameter>
        <condition>flatfieldDatasetId!=null</condition>
    </jobOptions>
    <jobOptions>
        <name>View flats (full frame, overlay channels)</name>
        <groupName>View type</groupName>
        <type>boolean</type>
        <programParameter>--option=flats-fullframe-overlay</programParameter>
        <condition>flatfieldDatasetId!=null</condition>
    </jobOptions>
    <jobOptions>
        <name>View whitelights</name>
        <groupName>View type</groupName>
        <type>boolean</type>
        <programParameter>--whitelights</programParameter>
        <condition>numWhitelightFiles&gt;0</condition>
    </jobOptions>
    <jobOptions>
        <name>View whitelights (full frame)</name>
        <groupName>View type</groupName>
        <type>boolean</type>
        <programParameter>--option=whitelights-fullframe</programParameter>
        <condition>numWhitelightFiles&gt;0</condition>
    </jobOptions>
    <jobOptions>
        <name>View whitelights (full frame, overlay channels)</name>
        <groupName>View type</groupName>
        <type>boolean</type>
        <programParameter>--option=whitelights-fullframe-overlay</programParameter>
        <condition>numWhitelightFiles&gt;0</condition>
    </jobOptions>
    <jobOptions>
        <name>View darks</name>
        <groupName>View type</groupName>
        <type>boolean</type>
        <programParameter>--darks</programParameter>
        <condition>darkDatasetId!=null</condition>
    </jobOptions>
    <jobOptions>
        <name>View darks (full frame)</name>
        <groupName>View type</groupName>
        <type>boolean</type>
        <programParameter>--option=darks-fullframe</programParameter>
        <condition>darkDatasetId!=null</condition>
    </jobOptions>
    <jobOptions>
        <name>View bias</name>
        <groupName>View type</groupName>
        <type>boolean</type>
        <programParameter>--option=biases</programParameter>
        <condition>biasDatasetId!=null</condition>
    </jobOptions>
    <jobOptions>
        <name>View bias (full frame)</name>
        <groupName>View type</groupName>
        <type>boolean</type>
        <programParameter>--option=biases-fullframe</programParameter>
        <condition>biasDatasetId!=null</condition>
    </jobOptions>
    <jobOptions>
        <name>Do not load traces</name>
        <type>boolean</type>
        <programParameter>--no-tracks</programParameter>
    </jobOptions>
    <jobOptions>
        <name>Track method</name>
        <type>enumeration</type>
        <programParameter>--trackmethod</programParameter>
        <values></values>
        <values>Simple</values>
        <values>SLH</values>
        <values>Biggles</values>
        <values>Simulation</values>
    </jobOptions>
    <jobOptions>
        <name>Do not clean levels/stats</name>
        <type>integer</type>
        <programParameter>--Levels.no-clean</programParameter>
        <defaultValue>0</defaultValue>
    </jobOptions>
    <jobOptions>
        <name>Quantum efficiency</name>
        <type>float</type>
        <programParameter>--EMCCD.qe</programParameter>
        <defaultValue>0.910000026</defaultValue>
    </jobOptions>
</jobType>
